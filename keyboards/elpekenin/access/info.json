{
    "keyboard_name": "access",
    "manufacturer": "elpekenin",
    "maintainer": "elpekenin",
    "url": "https://github.com/elpekenin/access_kb",
    "processor": "RP2040",
    "bootloader": "rp2040",
    "bootloader_instructions": "Press the physical BOOT button on the Raspberry Pi Pico, hold the top-left key when plugging in the board. Can also use QK_BOOT if mapped anywhere",
    "debounce": 5,
    "diode_direction": "COL2ROW",
    "features": {
        "audio": true,
        "backlight": false,
        "bootmagic": true,
        "console": true,
        "encoder": false,
        "mousekey": true,
        "extrakey": true,
        "nkro": false,
        "quantum_painter": false,
        "rgb_matrix": true,
        "unicode": false,
        "usbpd": false,
        "wpm": false
    },
    "matrix_pins": {
        "cols": ["GP16"],
        "rows": ["GP17"]
    },
    "usb": {
        "device_version": "1.0.0",
        "pid": "0x0001",
        "vid": "0xFEED"
    },
    "layouts": {
        "LAYOUT_ortho_1x1": {
            "layout": [
                { "matrix": [0, 0], "x": 0, "y": 0 }
            ]
        }
    },
    "rgb_matrix": {
        "layout": [
            { "flags": 4, "x":   0, "y":   0 , "matrix": [0, 0] },
            { "flags": 4, "x":  16, "y":   0 },
            { "flags": 4, "x":  32, "y":   0 },
            { "flags": 4, "x":  48, "y":   0 },

            { "flags": 4, "x":   0, "y":  16 },
            { "flags": 4, "x":  16, "y":  16 },
            { "flags": 4, "x":  32, "y":  16 },
            { "flags": 4, "x":  48, "y":  16 },

            { "flags": 4, "x":   0, "y":  32 },
            { "flags": 4, "x":  16, "y":  32 },
            { "flags": 4, "x":  32, "y":  32 },
            { "flags": 4, "x":  48, "y":  32 },

            { "flags": 4, "x":   0, "y":  48 },
            { "flags": 4, "x":  16, "y":  48 },
            { "flags": 4, "x":  32, "y":  48 },
            { "flags": 4, "x":  48, "y":  48 }
        ]
    }
}
